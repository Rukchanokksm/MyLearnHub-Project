{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react", "unused-imports"],
    "rules": {
      "react/prop-types": "off",
      "prettier/prettier": [
        "error",
        {
        //   "singleQuote": true,
          "trailingComma": "all",
          "endOfLine": "auto",
          "semi": false,
          "printWidth": 120
        }
      ],
      "no-unused-vars": "off",
      "unused-imports/no-unused-imports": "error"
    }
  }
  